---
const {class: className} = Astro.props;
---


<div class={className}>
    <div class="menu-icon-part"></div>
    <div class="menu-icon-part"></div>
    <div class="menu-icon-part"></div>
</div>

<style>
    .menu-icon-from-width{
        position: absolute;
        width: 100%;
        aspect-ratio: 1/1;

    }
    .menu-icon-from-height{
        position: absolute;
        height: 100%;
        aspect-ratio: 1/1;
    }
    .menu-icon-part{
        position: relative;
        width: 100%;
        height: 15%;
        background-color: white;
        margin-top: 13%;
        border-radius: 50vh;
    }
</style>